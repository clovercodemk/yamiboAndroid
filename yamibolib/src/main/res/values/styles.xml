    <style name="CustomButtonMargin">
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:layout_marginLeft">3dp</item>
        <item name="android:layout_marginRight">3dp</item>
        <item name="android:layout_marginBottom">4dp</item>
    </style>

    <style name="CustomSmallButtonMargin">
        <item name="android:layout_marginLeft">2dp</item>
        <item name="android:layout_marginRight">2dp</item>
        <item name="android:layout_marginBottom">3dp</item>
    </style>

    <style name="TitleBar">
        <item name="android:paddingLeft">0dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:background">@drawable/title_background</item>
    </style>

    <style name="TitleBarTitleView">
        <item name="android:textColor">@color/titlebar_main_title_text_color</item>
        <item name="android:textSize">@dimen/text_size_18</item>
    </style>

    <style name="TitleBarArrrowTitleView" parent="TitleBarTitleView"></style>

    <style name="TitleBarSubTitleView">
        <item name="android:textColor">#323232</item>
        <item name="android:textSize">@dimen/text_medium</item>
    </style>

